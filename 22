import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    Scanner scanner = new Scanner(System.in);
	    String[] num = new String[3];
	    
	    num[0] = scanner.next();
	    num[1] = scanner.next();
	    num[2] = scanner.next();
	    
	    Arrays.sort(num);
	    
	    int n = num[2] - (num[0] + num[1]);
	    
	    if (n > 0)
	        System.out.println(True);
	    else
	        System.out.println(False);
		
	}
}
